<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="store.store.contrtollers.TransactionsController">
   <children>
      <TableView fx:id="transactionTable" layoutX="108.0" layoutY="222.0" prefHeight="302.0" prefWidth="584.0">
        <columns>
          <TableColumn fx:id="clientTransactionTable" prefWidth="112.0" text="Klient" />
            <TableColumn fx:id="shopTransactionTable" prefWidth="111.0" text="Sklep" />
            <TableColumn fx:id="bookTransactionTable" prefWidth="121.0" text="Książka" />
            <TableColumn fx:id="dateTransactionTable" prefWidth="129.0" text="Data" />
            <TableColumn fx:id="priceTransactionTable" prefWidth="110.0" text="Cena" />
        </columns>
      </TableView>
      <Label layoutX="236.0" layoutY="14.0" prefHeight="93.0" prefWidth="328.0" text="Transackcje">
         <font>
            <Font size="64.0" />
         </font>
      </Label>
      <Button layoutX="637.0" layoutY="40.0" mnemonicParsing="false" onAction="#backButton" prefHeight="42.0" prefWidth="110.0" text="Wróć">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="21.0" mnemonicParsing="false" onAction="#byMonth" prefHeight="51.0" prefWidth="158.0" text="Miesięczne">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="91.0" mnemonicParsing="false" onAction="#mostPopularButton" prefHeight="93.0" prefWidth="158.0" text="Najchętniej kupowane" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
